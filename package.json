{
  "author": {
    "name": "Villem Alango",
    "email": "villem.alango@gmail.com"
  },
  "name": "dwalker",
  "version": "0.8.1",
  "description": "A rules-based file directory walker",
  "repository": {
    "type": "git",
    "url": "git://github.com/valango/duke.git"
  },
  "main": "src/DirWalker.js",
  "scripts": {
    "clean": "rimraf .nyc_output out reports t*mp *.log *.t*mp *.lcov",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint --ext .js .",
    "purge": "npm run clean && rimraf node_modules",
    "test": "nyc mocha 'test/**/*.spec.js'"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/valango/duke/issues"
  },
  "homepage": "https://github.com/valango/duke/blob/master/README.md",
  "engines": {
    "node": ">= 12.12.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "keywords": [
    "class-based",
    "glob",
    "directory",
    "filesystem",
    "fs",
    "rule-based",
    "traversal",
    "walking",
    "non-recursive"
  ],
  "dependencies": {
    "brace-expansion": "^1.1.11",
    "lodash.defaults": "^4.2.0",
    "sincere": "^1.0.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "codecov": "^3.6.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "lodash": "^4.17.15",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.0"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "exclude": [
      "src/definitions.js",
      "src/index.js"
    ],
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./reports"
  }
}
