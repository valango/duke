{
  "author": {
    "name": "Villem Alango",
    "email": "villem.alango@gmail.com"
  },
  "name": "dwalker",
  "version": "4.1.1",
  "description": "A rules-based file directory walker",
  "repository": {
    "type": "git",
    "url": "git://github.com/valango/duke.git"
  },
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf .nyc_output out reports t*mp *.log *.t*mp *.lcov",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "doc": "npm run doc1 && npm run doc2 && npm run doc3 && npm run doc4",
    "doc1": "jsdoc2md -c jsdoc.json src/index.js > doc/api.md && jsdoc2md -c jsdoc.json src/constants.js > doc/constants.md",
    "doc2": "jsdoc2md -c jsdoc.json src/R*.js > doc/Ruler.md && jsdoc2md -c jsdoc.json src/W*.js > doc/Walker.md",
    "doc3": "jsdoc2md  -c jsdoc.json src/l*.js >> doc/utils.md",
    "doc4": "jsdoc2md -c jsdoc.json src/r*.js >> doc/utils.md && jsdoc2md -c jsdoc.json src/t*.js >> doc/utils.md",
    "lint": "eslint --ext .js .",
    "purge": "npm run clean && rimraf node_modules",
    "t": "nyc mocha 'test/**/Walker.spec.js'",
    "test": "eslint --ext .js . && nyc mocha 'test/**/*.spec.js'"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/valango/duke/issues"
  },
  "homepage": "https://github.com/valango/duke/blob/master/README.md",
  "engines": {
    "node": ">= 12.12.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "keywords": [
    "class-based",
    "glob",
    "directory",
    "filesystem",
    "fs",
    "rule-based",
    "traversal",
    "walking",
    "non-recursive"
  ],
  "dependencies": {
    "brace-expansion": "^1.1.11",
    "sincere": "^2.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc-to-markdown": "^5.0.3",
    "lodash": "^4.17.15",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.0"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "exclude": [
      "src/index.js"
    ],
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./reports"
  }
}
