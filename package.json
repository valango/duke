{
  "name": "duke",
  "version": "0.1.0",
  "description": "A rules-based file directory walker",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js .",
    "test": "cross-env NODE_ENV=test nyc mocha 'test/**/*.spec.js'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "brace-expansion": "^1.1.11",
    "lodash.clone": "^4.5.0",
    "lodash.defaults": "^4.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "lodash": "^4.17.15",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "exclude": [],
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./reports"
  }
}
