{
  "author": {
    "name": "Villem Alango",
    "email": "villem.alango@gmail.com"
  },
  "name": "dwalker",
  "version": "4.1.1",
  "description": "Asynchronous rule-based file system walker",
  "repository": {
    "type": "git",
    "url": "git://github.com/valango/duke.git"
  },
  "files": [
    "doc/*.md",
    "README.md",
    "LICENSE",
    "index.js",
    "relativize.js",
    "src/*.js",
    "symlinksFinal.js"
  ],
  "scripts": {
    "clean": "rimraf .nyc_output out reports t*mp *.log *.t*mp *.lcov",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint --ext .js .",
    "purge": "npm run clean && rimraf node_modules",
    "test": "npm run lint && nyc mocha 'test/**/*.spec.js'"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/valango/duke/issues"
  },
  "homepage": "https://github.com/valango/duke/blob/master/README.md",
  "engines": {
    "node": ">= 12.12.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "keywords": [
    "async",
    "asynchronous",
    "class-based",
    "glob",
    "directory",
    "filesystem",
    "fs",
    "rule-based",
    "traversal",
    "walking",
    "non-recursive"
  ],
  "dependencies": {
    "brace-expansion": "^1.1.11",
    "lodash.omit": "^4.5.0",
    "sincere": "^2.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "codecov": "^3.8.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.1.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.0"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "exclude": [
      "src/typedefs.js"
    ],
    "include": [
      "src/**/*.js",
      "stubs/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./reports"
  }
}
